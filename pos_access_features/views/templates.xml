<odoo>
    <data>
        <template id="assets_backend" name="pos assets" inherit_id="point_of_sale.assets">
            <xpath expr="." position="inside">

                <script type="text/javascript" src="/pos_access_features/static/src/js/feature.js"></script>
            </xpath>

            <xpath expr="//link[@id='pos-stylesheet']" position="after">
              <link rel="stylesheet" href="/pos_access_features/static/src/css/pos.css"/>
          </xpath>
        </template>
    </data>
</odoo>